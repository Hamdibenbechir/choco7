<script>
const login = document.getElementById("login");
const mybtn = document.getElementById("mybtnpro");

// Check if the popup should be shown
if (!localStorage.getItem("discountReceived")) {
  setTimeout(() => {
    login.style.display = "flex";
  }, 7000); // Show popup after 7 seconds
}

// Close the popup when the close button is clicked
mybtn.addEventListener("click", (e) => {
  e.preventDefault();
  login.style.display = "none";
});

// Handle the discount form submission
document.getElementById("discountForm").addEventListener("submit", function (e) {
  e.preventDefault(); // Prevent the form from reloading the page

  const emailInput = document.getElementById("emailInput").value;

  // Validate email
  if (!emailInput || !/\S+@\S+\.\S+/.test(emailInput)) {
    alert("Please enter a valid email address.");
    return;
  }

  // Store a flag in localStorage to prevent the popup from showing again
  localStorage.setItem("discountReceived", "true");

  // Optionally, send the email to a backend for storage
  // Example POST request:
  // fetch("/save-email", { method: "POST", body: JSON.stringify({ email: emailInput }) });

  // Redirect to Shopify with the discount code applied
  window.location.href = "/discount/20OFF"; // Redirects to the discount page

  // Optionally show a success message
  const discountMessage = document.getElementById("discountMessage");
  discountMessage.style.display = "block";
});


</script>

